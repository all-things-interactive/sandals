{
    "cmd": [ "sh", "-xc", "ifconfig;hostname;pstree -p;cat /proc/self/cgroup;ls /proc/1/fd;exit 42" ],
    "pipes": [
        { "file": "/dev/stdout", "stdout": true },
        { "file": "/dev/stderr", "stderr": true }
    ],
    "mounts": [
        { "type": "tmpfs", "dest": "/tmp" },
        { "type": "proc", "dest": "/proc" }
    ]
}
